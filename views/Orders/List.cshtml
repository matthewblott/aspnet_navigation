@model IEnumerable<Order>

<table class="table">
  <tr>
    <th scope="col">Id</th>
    <th scope="col">User Id</th>
    <th scope="col"></th>
  </tr>
  @foreach (var item in Model)
  {
    <tr>
      <td>
        @item.Id
      </td>
      <td>
        @item.UserId
      </td>
      <td>
        <a asp-controller="Orders" asp-action="Show" asp-route-orderId="@item.Id">Show</a>
      </td>
    </tr>
  }
  
</table>