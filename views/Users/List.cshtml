@model IEnumerable<User>

<table class="table">
  <tr>
    <th scope="col">Id</th>
    <th scope="col">Userame</th>
    <th scope="col">Password</th>
    <th scope="col"></th>
  </tr>
  @foreach (var item in Model)
  {
    <tr>
      <td>
        @item.Id
      </td>
      <td>
        @item.Username
      </td>
      <td>
        @item.Password
      </td>
      <td>
        <a asp-controller="Users" asp-action="Edit" asp-route-userId="@item.Id">Edit</a>
      </td>

    </tr>
  }

</table>